<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="startMenu" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TicTacToeGame.TicTacGUIController">
    <VBox alignment="CENTER" layoutY="-42.0" prefHeight="600" prefWidth="600">
        <children>
            <Label text="Welcome to Tic Tac Toe" />
            <Button fx:id="onePlayerMode" onAction="#PlayerOneMode" stylesheets="@style.css" text="ONE PLAYER" />
            <Button fx:id="twoPlayerMode" onAction="#PlayerTwoMode" stylesheets="@style.css" text="TWO PLAYER" />
            <Button fx:id="quitButton" onAction="#logout" stylesheets="@style.css" text="QUIT" />
        </children>
    </VBox>
</AnchorPane>
