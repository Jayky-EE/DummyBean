<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="startMenu" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TicTacToeGame.TicTacGUIController">
   <children>
      <TextField fx:id="PlayerOneTextField" alignment="CENTER" layoutX="220.0" layoutY="240.0" />
      <Label fx:id="PlayerOneLabel" alignment="CENTER" layoutX="220.0" layoutY="200.0" text="ENTER PLAYER ONE NAME:" />
      <Button fx:id="submitButton" layoutX="380.0" layoutY="240.0" mnemonicParsing="false" onAction="#submit" stylesheets="@../stylesheets/style.css" text="SUBMIT" />
   </children>
</AnchorPane>
